FROM Immatricolazioni_2021 AS im, Occupazione_Popolazione AS op
WHERE im.Provincia=op.Provincia
ORDER BY (im.EL*10000)/Tot_occu ASC;</sql><sql name="Lavoratori_15-24-Immatricolazioni_Eelettrico">SELECT im.Provincia, im.EL, im.EL*10000/M_Y15_24_occup, im.EL*10000/F_Y15_24_occup, M_Y15_24_occup+F_Y15_24_occup as somma
FROM Immatricolazioni_2021 AS im, Occupazione_Popolazione AS op
WHERE im.Provincia=op.Provincia
ORDER BY (im.EL*10000)/Tot_occu ASC;</sql><sql name="Lavoratori_25-49-Immatricolazioni_Eelettrico">SELECT im.Provincia, im.EL, im.EL*10000/M_Y25_49_occup, im.EL*10000/F_Y25_49_occup, M_Y25_49_occup+F_Y25_49_occup as somma
FROM Immatricolazioni_2021 AS im, Occupazione_Popolazione AS op
WHERE im.Provincia=op.Provincia
ORDER BY (im.EL*10000)/Tot_occu ASC;</sql><sql name="Lavoratori_50-64-Immatricolazioni_Eelettrico">SELECT im.Provincia, im.EL, im.EL*10000/M_Y50_64_occup, im.EL*10000/F_Y50_64_occup, M_Y50_64_occup+F_Y50_64_occup as somma
FROM Immatricolazioni_2021 AS im, Occupazione_Popolazione AS op
WHERE im.Provincia=op.Provincia
ORDER BY (im.EL*10000)/Tot_occu ASC;</sql><sql name="Lavoratori-Immatricolazioni Benzina 2021">SELECT im.Provincia, im.BE, im.BE*10000/Tot_occu, im.BE*10000/Tot_pop
FROM Immatricolazioni_2021 AS im, Occupazione_Popolazione AS oc
WHERE im.Provincia=oc.Provincia
ORDER BY (im.BE*10000)/Tot_occu ASC;</sql><sql name="Lavoratori-Immatricolazioni Gasolio 2021">SELECT im.Provincia, im.GA, im.GA*10000/Tot_occu
FROM Immatricolazioni_2021 AS im, Occupazione_Popolazione AS oc
WHERE im.Provincia=oc.Provincia
ORDER BY (im.GA*10000)/Tot_occu ASC;</sql><sql name="Immatricolazioni per Regione">SELECT Regione, SUM (EL), SUM(BE), SUM(GA)
FROM Immatricolazioni_2021 AS im JOIN Italia
	ON im.Provincia=Italia.Provincia
GROUP BY Regione; </sql><sql name="Occupazione_Regione">CREATE VIEW Occupazione_Regione AS
SELECT Regione, sum(tot_pop) as tot_pop_reg
FROM  Italia JOIN Occupazione_Popolazione on Italia.provincia=Occupazione_Popolazione.provincia
GROUP BY Regione</sql><sql name="SQL 5">SELECT ita.Regione, tot_pop_reg*anno2019
FROM Inquinamento_trasporto_abitante AS ita JOIN Occupazione_Regione as ocr
ON ita.Regione=ocr.Regione
GROUP BY ita.Regione
ORDER BY tot_pop_reg*anno2019 DESC;</sql><current_tab id="8"/></tab_sql></sqlb_project>
